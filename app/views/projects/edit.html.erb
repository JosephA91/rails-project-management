
  <div class="row">
    <div class="col-md-6">
      <div class="default-block">
      <h2>Edit Project</h2>
        <div class="line-dark"></div>
        <%= render partial: "messages/errors", locals: {resource: @project} %>
        <%= render partial: "edit_form", locals: {project: @project} %>
      </div>
    </div>
    <div class="col-md-6"> 
      <div class="default-block">
        <h3>Edit Collaborators</h3>
        <div class="line-dark"></div>

        
        <% @collaborators.each do |collaborator, index| %>
          <div class="row">
            <div class="col-xs-6">
              <%= render partial: "edit_collaborators", locals: {collaborator: collaborator, index: index} %>
            </div>
            <div class="col-xs-6">
              
            </div>
          </div>
        <%end %>
          

      </div>
    </div>
  </div>
</div>