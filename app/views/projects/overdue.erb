<%= render partial: "projects_filter", locals:{overdue: @overdue, active: @active, complete: @complete} %>


<div class="default-block">
  <h2>Overdue Projects</h2>
  <div class="line-dark"></div>
  <% if @projects.empty? %>
    <h4><%= "You don't have any late projects. Good job!" %></h4>
  <% else %>
    <%= render @projects %>
  <% end %>
</div>
